<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    android:id="@+id/fragment_page_rootview"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!-- Header name page -->
    <LinearLayout
        android:id="@+id/layout_header_param"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/width_header_name_page"
        android:orientation="horizontal"
        android:padding="@dimen/width_header_name_page">


        <TextView
            style="@style/titre_accueil"
            android:id="@+id/fragment_page_title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="coucou"/>
    </LinearLayout>


</FrameLayout>